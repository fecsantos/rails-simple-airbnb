<%= link_to 'Add Flat', new_flat_path %>

<h1>All our Flats (<%= @flats.count %>)</h1>

<div class="flat-cards">
<% @flats.each do |flat| %>
  <div class="flat-card">
    <img src="data:image/jpeg;base64,">
    <h5><%= flat.name %></h5>
    <p><%= flat.address %></p>
    <div class="price-guests">
      <p>guests <%= flat.number_of_guests %></p>
      <p>price <%= flat.price_per_night %></p>
    </div>
    <p><%= link_to 'More Info', flat_path(flat) %> <%= link_to 'Edit', edit_flat_path(flat) %> <%= link_to 'Remove', flat_path(flat), method: :delete,  data: { confirm: "Are you sure?" }  %></p>
  </div>
<% end %>
</div>
